package main

import (
    "github.com/Chronostasys/calc/runtime/generator"
)



func testGenerator() generator.Generator<int> {
    a := 1
    yield a
    a = a + 1
    yield a
    a = 100
    f := func () int {
        a = a + 1
        return 100
    }
    f()
    fg := func () generator.Generator<int> {
        a := 1
        yield a
        a = a + 1
        yield a
        a = 100
        yield a
    }
    gen := fg()
    for ;gen.StepNext(); {
        printIntln(gen.GetCurrent())
    }
    yield a
}

func rungenerator() void {
    gen := testGenerator()
    for ;gen.StepNext(); {
        printIntln(gen.GetCurrent())
    }
    return
}

