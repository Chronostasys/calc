package main

import (
    "github.com/Chronostasys/calc/runtime/generator"
)



func testGenerator() generator.Generator<int> {
    a := 1
    yield a
    a = a + 1
    yield a
    a = 100
    f := func () int {
        a = a + 1
        return 100
    }
    f()
    fg := func () generator.Generator<int> {
        yield a
        a = a + 1
        yield a
        a = a * 2
        yield a
    }
    gen := fg()
    for ;gen.StepNext(); {
        printIntln(gen.GetCurrent())
    }
    fg1 := func () generator.Generator<float> {
        yield 0.1
        a = a + 1
        yield 0.1
        a = a * 2
        yield 0.1
    }
    gen1 := fg1()
    for ;gen1.StepNext(); {
        printFloatln(gen1.GetCurrent())
    }

    yield a
}

func rungenerator() void {
    gen := testGenerator()
    for ;gen.StepNext(); {
        printIntln(gen.GetCurrent())
    }
    return
}

