func main() int {
    for {
        loop()
    }
    return 0
}

func massiveAllocation() [100]int {
    d := [100]int{}
    return d
}
func loop() void {
    for i:=0;i<1000;i=i+1{
        massiveAllocation()
    }
    return
}

func heapalloc<T>() *T {
    size := sizeof<T>()
    bs := GC_malloc(size)
    seted := memset(bs,0,size)
    return unsafecast<*byte,*T>(seted)
}